{
  "name": "webappengine",
  "version": "1.1.0",
  "description": "A web application platform that can host multiple web apps running with Node.js.",
  "homepage": "https://github.com/cheton/webappengine",
  "author": "Cheton Wu <cheton@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=0.10.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cheton/webappengine"
  },
  "keywords": [
    "webappengine"
  ],
  "bin": {
    "webappengine": "./bin/webappengine"
  },
  "main": "./index.js",
  "scripts": {
    "prepublish": "npm run clean && npm run lint && npm run build",
    "prebuild": "bash ./scripts/prebuild.sh",
    "prebuild-dev": "npm run prebuild",
    "prebuild-prod": "npm run prebuild",
    "build": "gulp prod",
    "build-dev": " gulp dev",
    "build-prod": "gulp prod",
    "clean": "bash -c 'rm -rf ./dist ./output'",
    "start": "./bin/main",
    "predev": "bash ./watchpack-ignore-node-modules.sh",
    "dev": "npm run build-dev && NODE_ENV=\"development\" ./bin/main -vv",
    "prod": "npm run build-prod && NODE_ENV=\"production\" ./bin/main",
    "lint": "npm run eslint && npm run stylint",
    "eslint": "eslint *.js src bin gulp test",
    "stylint": "stylint src/web",
    "test": "node babel-tap --coverage test/*.js",
    "coveralls": "node babel-tap --coverage --coverage-report=text-lcov test/*.js | node_modules/.bin/coveralls"
  },
  "dependencies": {
    "body-parser": "~1.15.0",
    "chalk": "~1.1.3",
    "commander": "~2.9.0",
    "compression": "~1.6.1",
    "connect-multiparty": "~2.0.0",
    "connect-restreamer": "~1.0.3",
    "consolidate": "~0.14.1",
    "cookie-parser": "~1.4.1",
    "del": "~2.2.0",
    "errorhandler": "~1.4.3",
    "express": "~4.13.4",
    "express-session": "~1.13.0",
    "hogan.js": "~3.0.2",
    "i18next": "~2.5.1",
    "i18next-express-middleware": "~1.0.1",
    "i18next-node-fs-backend": "~0.1.2",
    "jsuri": "~1.3.1",
    "lodash": "~4.11.1",
    "method-override": "~2.3.5",
    "morgan": "~1.7.0",
    "multiparty": "~4.1.2",
    "serve-favicon": "~2.3.0",
    "serve-static": "~1.10.2",
    "session-file-store": "~0.2.0",
    "winston": "~2.2.0"
  },
  "devDependencies": {
    "babel-cli": "~6.8.0",
    "babel-core": "~6.8.0",
    "babel-eslint": "~6.0.0-beta.6",
    "babel-loader": "~6.2.4",
    "babel-plugin-transform-decorators-legacy": "~1.3.4",
    "babel-preset-es2015": "~6.6.0",
    "babel-preset-stage-0": "~6.5.0",
    "bootstrap": "~3.3.6",
    "coveralls": "~2.11.8",
    "css-loader": "~0.23.1",
    "eslint": "~2.9.0",
    "eslint-config-airbnb": "~8.0.0",
    "eslint-loader": "~1.3.0",
    "eslint-plugin-import": "^1.7.0",
    "eslint-plugin-jsx-a11y": "~1.0.4",
    "eslint-plugin-react": "~5.0.1",
    "eventsource-polyfill": "~0.9.6",
    "file-loader": "~0.8.5",
    "gulp": "~3.9.0",
    "gulp-babel": "~6.1.2",
    "gulp-util": "~3.0.3",
    "json-loader": "~0.5.4",
    "nib": "~1.1.0",
    "require-dir": "~0.3.0",
    "run-sequence": "~1.1.5",
    "style-loader": "~0.13.0",
    "stylint": "~1.3.9",
    "stylint-loader": "~1.0.0",
    "stylus": "~0.54.2",
    "stylus-loader": "~2.0.0",
    "tap": "~5.7.0",
    "transform-loader": "~0.2.3",
    "url-loader": "~0.5.7",
    "webpack": "~1.13.0",
    "webpack-dev-middleware": "~1.6.1",
    "webpack-hot-middleware": "~2.10.0"
  }
}
